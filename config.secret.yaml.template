# 敏感配置信息
# 请将此文件复制为config.secret.yaml并填入您的敏感信息
# 确保config.secret.yaml不会被提交到版本控制系统中

# 邮件账户配置
email:
  username: "your_email@example.com"  # 替换为您的邮箱用户名
  password: "your_secure_password_here"  # 替换为您的邮箱密码

# 钉钉机器人配置
dingtalk:
  webhook_url: "https://oapi.dingtalk.com/robot/send?access_token=your_access_token"  # 替换为您的钉钉机器人Webhook地址
  secret: "your_dingtalk_secret"  # 替换为您的钉钉机器人安全设置的加签密钥

# AI分析器API密钥配置
ai_analyzer:
  api_key: "your_api_key_here"  # 替换为您的API密钥，例如
